<?xml version="1.0" encoding="UTF-8" ?><!DOCTYPE log4j:configuration SYSTEM "log4j.dtd"><log4j:configuration xmlns:log4j="http://jakarta.apache.org/log4j/">	<appender name="CONSOLE" class="org.apache.log4j.ConsoleAppender">		<layout class="org.apache.log4j.PatternLayout">			<param name="ConversionPattern" value="%d{yyyy-MM-dd HH:mm:ss} [%-5p] [%t] [%c] - %m%n" />		</layout>		<!--限制输出级别 -->		<filter class="org.apache.log4j.varia.LevelRangeFilter">			<param name="LevelMax" value="ERROR" />			<param name="LevelMin" value="INFO" />		</filter>	</appender>	<appender name="ROOTFILE" class="org.apache.log4j.RollingFileAppender">		<param name="File" value="logs/root.log" />		<param name="MaxFileSize" value="5MB" />		<param name="MaxBackupIndex" value="3" />		<layout class="org.apache.log4j.PatternLayout">			<param name="ConversionPattern" value="%d{yyyy-MM-dd HH:mm:ss.SSS} [%-5p] [%t] [%c{1}] - %m%n" />		</layout>	</appender>	<appender name="AgreeFinalEstimateJob" class="org.apache.log4j.RollingFileAppender">			<param name="File" value="logs/AgreeFinalEstimateJob.log" />			<param name="MaxFileSize" value="5MB" />			<param name="MaxBackupIndex" value="3" />			<layout class="org.apache.log4j.PatternLayout">				<param name="ConversionPattern" value="%d{yyyy-MM-dd HH:mm:ss.SSS} [%-5p] [%t] [%c{1}] - %m%n" />			</layout>		</appender>		<appender name="CancelContractJob" class="org.apache.log4j.RollingFileAppender">			<param name="File" value="logs/CancelContractJob.log" />			<param name="MaxFileSize" value="5MB" />			<param name="MaxBackupIndex" value="3" />			<layout class="org.apache.log4j.PatternLayout">				<param name="ConversionPattern" value="%d{yyyy-MM-dd HH:mm:ss.SSS} [%-5p] [%t] [%c{1}] - %m%n" />			</layout>		</appender>		<appender name="CompanyAuthApplyJob" class="org.apache.log4j.RollingFileAppender">			<param name="File" value="logs/CompanyAuthApplyJob.log" />			<param name="MaxFileSize" value="5MB" />			<param name="MaxBackupIndex" value="3" />			<layout class="org.apache.log4j.PatternLayout">				<param name="ConversionPattern" value="%d{yyyy-MM-dd HH:mm:ss.SSS} [%-5p] [%t] [%c{1}] - %m%n" />			</layout>		</appender>		<appender name="ContractConfirmJob" class="org.apache.log4j.RollingFileAppender">			<param name="File" value="logs/ContractConfirmJob.log" />			<param name="MaxFileSize" value="5MB" />			<param name="MaxBackupIndex" value="3" />			<layout class="org.apache.log4j.PatternLayout">				<param name="ConversionPattern" value="%d{yyyy-MM-dd HH:mm:ss.SSS} [%-5p] [%t] [%c{1}] - %m%n" />			</layout>		</appender>		<appender name="ContractConfirmGoodsInfoJob" class="org.apache.log4j.RollingFileAppender">			<param name="File" value="logs/ContractConfirmGoodsInfoJob.log" />			<param name="MaxFileSize" value="5MB" />			<param name="MaxBackupIndex" value="3" />			<layout class="org.apache.log4j.PatternLayout">				<param name="ConversionPattern" value="%d{yyyy-MM-dd HH:mm:ss.SSS} [%-5p] [%t] [%c{1}] - %m%n" />			</layout>		</appender>		<appender name="ContractMatchJob" class="org.apache.log4j.RollingFileAppender">			<param name="File" value="logs/ContractMatchJob.log" />			<param name="MaxFileSize" value="5MB" />			<param name="MaxBackupIndex" value="3" />			<layout class="org.apache.log4j.PatternLayout">				<param name="ConversionPattern" value="%d{yyyy-MM-dd HH:mm:ss.SSS} [%-5p] [%t] [%c{1}] - %m%n" />			</layout>		</appender>		<appender name="DataCountAllUserJob" class="org.apache.log4j.RollingFileAppender">			<param name="File" value="logs/DataCountAllUserJob.log" />			<param name="MaxFileSize" value="5MB" />			<param name="MaxBackupIndex" value="3" />			<layout class="org.apache.log4j.PatternLayout">				<param name="ConversionPattern" value="%d{yyyy-MM-dd HH:mm:ss.SSS} [%-5p] [%t] [%c{1}] - %m%n" />			</layout>		</appender>		<appender name="ContractEvaluationJob" class="org.apache.log4j.RollingFileAppender">			<param name="File" value="logs/ContractEvaluationJob.log" />			<param name="MaxFileSize" value="5MB" />			<param name="MaxBackupIndex" value="3" />			<layout class="org.apache.log4j.PatternLayout">				<param name="ConversionPattern" value="%d{yyyy-MM-dd HH:mm:ss.SSS} [%-5p] [%t] [%c{1}] - %m%n" />			</layout>		</appender>		<appender name="ExpireContractJob" class="org.apache.log4j.RollingFileAppender">			<param name="File" value="logs/ExpireContractJob.log" />			<param name="MaxFileSize" value="5MB" />			<param name="MaxBackupIndex" value="3" />			<layout class="org.apache.log4j.PatternLayout">				<param name="ConversionPattern" value="%d{yyyy-MM-dd HH:mm:ss.SSS} [%-5p] [%t] [%c{1}] - %m%n" />			</layout>		</appender>		<appender name="OnlinePayJob" class="org.apache.log4j.RollingFileAppender">			<param name="File" value="logs/OnlinePayJob.log" />			<param name="MaxFileSize" value="5MB" />			<param name="MaxBackupIndex" value="3" />			<layout class="org.apache.log4j.PatternLayout">				<param name="ConversionPattern" value="%d{yyyy-MM-dd HH:mm:ss.SSS} [%-5p] [%t] [%c{1}] - %m%n" />			</layout>		</appender>		<appender name="OrderFindJob" class="org.apache.log4j.RollingFileAppender">			<param name="File" value="logs/OrderFindJob.log" />			<param name="MaxFileSize" value="5MB" />			<param name="MaxBackupIndex" value="3" />			<layout class="org.apache.log4j.PatternLayout">				<param name="ConversionPattern" value="%d{yyyy-MM-dd HH:mm:ss.SSS} [%-5p] [%t] [%c{1}] - %m%n" />			</layout>		</appender>		<appender name="ContractPayFundsJob" class="org.apache.log4j.RollingFileAppender">			<param name="File" value="logs/ContractPayFundsJob.log" />			<param name="MaxFileSize" value="5MB" />			<param name="MaxBackupIndex" value="3" />			<layout class="org.apache.log4j.PatternLayout">				<param name="ConversionPattern" value="%d{yyyy-MM-dd HH:mm:ss.SSS} [%-5p] [%t] [%c{1}] - %m%n" />			</layout>		</appender>	<!--通过<category></category>的定义可以将各个包中的类日志输出到不同的日志文件中 -->		<category name="com.appabc.datas.task.agreefinalestimate">			<level value="DEBUG" />			<appender-ref ref="AgreeFinalEstimateJob" />		</category>		<category name="com.appabc.datas.task.cancelcontract">			<level value="DEBUG" />			<appender-ref ref="CancelContractJob" />		</category>		<category name="com.appabc.datas.task.company">			<level value="DEBUG" />			<appender-ref ref="CompanyAuthApplyJob" />		</category>		<category name="com.appabc.datas.task.confirmcontract">			<level value="DEBUG" />			<appender-ref ref="ContractConfirmJob" />		</category>		<category name="com.appabc.datas.task.confirmgoodsinfo">			<level value="DEBUG" />			<appender-ref ref="ContractConfirmGoodsInfoJob" />		</category>		<category name="com.appabc.datas.task.contractmatch">			<level value="DEBUG" />			<appender-ref ref="ContractMatchJob" />		</category>		<category name="com.appabc.datas.task.datacount">			<level value="DEBUG" />			<appender-ref ref="DataCountAllUserJob" />		</category>		<category name="com.appabc.datas.task.evaluationcontract">			<level value="DEBUG" />			<appender-ref ref="ContractEvaluationJob" />		</category>		<category name="com.appabc.datas.task.expirecontract">			<level value="DEBUG" />			<appender-ref ref="ExpireContractJob" />		</category>		<category name="com.appabc.datas.task.onlinePay">			<level value="DEBUG" />			<appender-ref ref="OnlinePayJob" />		</category>		<category name="com.appabc.datas.task.orderfind">			<level value="DEBUG" />			<appender-ref ref="OrderFindJob" />		</category>		<category name="com.appabc.datas.task.payfunds.ContractPayFundsJob">			<level value="DEBUG" />			<appender-ref ref="ContractPayFundsJob" />		</category>	<root>		<priority value="ALL" />		<appender-ref ref="CONSOLE" />		<appender-ref ref="ROOTFILE" />	</root></log4j:configuration>